<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Breeze/OData</title>
    <link rel="shortcut icon" type="image/png" href="/app/images/logo.png">
    @Styles.Render("~/Content/css/bundle")
    @Scripts.Render("~/bundles/modernizr")

</head>
    <body>
        <div class="wrap">
            <header>
                <div class="navbar navbar-inverse navbar-fixed-top">
                    <div class="container">

                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>

                        </div>
                        <div class="navbar-collapse ">
                            <ul class="nav navbar-nav">
                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                <li>@Html.ActionLink("BOWA", "Bowa", "Home")</li>
                                <li>@Html.ActionLink("About", "About", "Home")</li>
                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                            </ul>
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>
            </header>
             <header>
                <div class="navbar navbar-inverse navbar-fixed-top">
                    <div class="navbar-inner">
                        <div class="container-fluid">
                            <a class="logo pull-left" href="/">
                                <img src="~/Content/img/BCAlogo-DNA.gif" height="38" width="52" title="BCA Research" />
                            </a>
                            <div id="topNavigation"></div>  
                            <ul class="nav navbar-nav">
                                <li>@Html.ActionLink("CRUD", "Index", "Home")</li>
                                <li>@Html.ActionLink("BOWA", "Bowa", "Home")</li>
                                <li>@Html.ActionLink("About", "About", "Home")</li>
                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                            </ul>
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>
            </header>
            <div class="body @ViewBag.BrowserVersion">
                @RenderBody()
            </div>
            <div class="push"></div>
        </div>
        <footer>
            @Html.Partial("~/Views/Shared/_Footer.cshtml")
        </footer>
        @*@Html.RequireJs("main.js")*@
        @RenderSection("scripts", required: false)
    </body>
</html>
